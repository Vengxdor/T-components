---
import DocsAside from '../components/doc/DocsAside.astro'
import Layout from './Layout.astro'

interface Props {
  title: string
}

const { title } = Astro.props
---

<Layout title={title}>
  <main class='flex gap-16'>
    <DocsAside />
    <div class='w-full'>
      <slot />
    </div>
  </main>
</Layout>

